    <component name="entity" moduleName="ucfbase" title="公用实体组件">
        <class name="BizObject" title="基类">
            <attributes>
                <attribute name="id" title="ID" type="String" iLength="64" isKey="true"/>
                <attribute name="pubts" columnName="pubts" title="时间戳" type="DateTime" isSyncKey="true"/>
            </attributes>
        </class>
    </component>

