                <attribute name="path" columnName="path" title="路径"
                           type="String" iLength="255" />
                <attribute name="sort" columnName="sort_num" title="排序号"
                           type="Integer" />
                <attribute name="isEnd" title="是否末级" type="Boolean" />
            </attributes>
        </interface>
